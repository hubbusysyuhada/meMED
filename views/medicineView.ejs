<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>
<nav>
    <a href="/">Home</a>
    <%- include('./partials/navbar.ejs')%>
</nav>
<h1>
    <%= title %>
</h1>

<body>
    <a href="/medicine/add"><button>Add Medicine</button></a>
    <table border="2">
        <tr>
            <th>Name</th>
            <th>Stock </th>
    
        </tr>
        <% data.map(obj=> { %>
            <tr>
                <td>
                    <%= obj.name%>
                </td>
                <td>
                    <%= obj.stock %>
                </td>
                <td>
                    <a href="/medicine/edit/<%= obj.id %>"><button>Edit</button></a>
                    <a href="/medicine/delete/<%= obj.id %>"
                        onclick="return confirm('Delete this data?')"><button>Delete</button></a>
                </td>
            </tr>
            <% }) %>
    </table>
</body>

</html>