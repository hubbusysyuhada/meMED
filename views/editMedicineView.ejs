    <title>
        <%= title %>
    </title>
</head>
<nav>
    <%- include ('./partials/navbar')%>
</nav>
<h1>
    <%= title %>
</h1>
<body>
    <form action="/medicine/edit/<%= data.id %>" method="POST">

        <label for="name">Name</label><br>
        <input type="text" name="name" id=""value="<%= undefined ? '' : data.name %>"><br>

        <label for="stock">Stock</label><br>
